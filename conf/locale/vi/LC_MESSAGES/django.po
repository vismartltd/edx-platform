#: lms/templates/verify_student/photo_reverification.html
msgid "Be well lit"
msgstr "Được đủ ánh sáng"

#: lms/templates/verify_student/photo_reverification.html
msgid "Show your whole face"
msgstr "Hiển thị toàn bộ khuôn mặt của bạn"

#: lms/templates/verify_student/photo_reverification.html
#: lms/templates/verify_student/photo_reverification.html
msgid "The photo on your ID must match the photo of your face"
msgstr ""
"Các  ID trên hình ảnh của bạn phải phù hợp với hình ảnh của khuôn mặt của "
"bạn"

#: lms/templates/verify_student/photo_reverification.html
msgid "Be readable (not too far away, no glare)"
msgstr "Có thể đọc được (không quá xa, không có ánh sáng chói)"

#: lms/templates/verify_student/photo_reverification.html
msgid "The name on your ID must match the name on your account below"
msgstr "Tên ID của bạn phải phù hợp với tên trên tài khoản của bạn dưới đây"

#: lms/templates/verify_student/photo_reverification.html
msgid "Photos don't meet the requirements?"
msgstr "Hình ảnh không đáp ứng được yêu cầu?"

